<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ChessboardDefault">
    	<item name="android:layout_width">fill_parent</item>
    	<item name="android:layout_height">wrap_content</item>
        <item name="notationTextSize">12sp</item>
        <item name="notationTextColor">#fff</item>
        <item name="whiteCellsColor">#fff</item>
        <item name="blackCellsColor">#000</item>
        <item name="cellsSize">35dp</item>
    </style>
    
    <style name="ChessboardClassicMarble">
    	<item name="android:layout_width">fill_parent</item>
    	<item name="android:layout_height">wrap_content</item>
        <item name="notationTextSize">12sp</item>
        <item name="notationTextColor">#000</item>
        <item name="whiteCellsBackground">@drawable/marble_white_cell</item>
        <item name="blackCellsBackground">@drawable/marble_black_cell</item>
        <item name="notationBackground">#fff</item>
        <item name="cellsSize">35dp</item>
    </style>
</resources>
